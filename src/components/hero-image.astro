---
import { Image } from "astro:assets";
import moonImage from "~/assets/heroimage.jpg";

const widths = [450, 800, 1200, 2720];
const sizes = "100vw";
---

<Image
  class="h-full w-full object-cover"
  src={moonImage}
  widths={widths}
  sizes={sizes}
  alt="An image of an astronaut gazing across a mountain range"
/>
